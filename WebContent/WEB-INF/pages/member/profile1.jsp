<div class="col-sm-offset-4">
	<table class="table">
		<tr>
			<td>User Name: </td>
			<td>{{profile.firstName}} {{profile.lastName}}</td>
		</tr>
		<tr>
			<td>Email Address: </td>
			<td>{{profile.email}}</td>
		</tr>
		<tr>
			<td>Address: </td>
			<td>{{profile.address}}</td>
		</tr>
		<tr>
			<td>City: </td>
			<td>{{profile.city}}</td>
		</tr>
		<tr>
			<td>State: </td>
			<td>{{profile.state}}</td>
		</tr>
		<tr>
			<td>Zip Code:</td>
			<td>{{profile.zipCode}}</td>
	</table>
	<div class="col-sm-offset-6">
	<button class="btn btn-default btn-sm">Edit</button>
	</div>
</div>